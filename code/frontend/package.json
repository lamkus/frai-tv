{
  "name": "remaike-tv-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "last 2 Chrome versions",
    "last 2 Safari versions",
    "last 2 Firefox versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx",
    "yt:pack": "node ./scripts/generate-youtube-metadata.mjs",
    "yt:scan-chapters": "node ./scripts/scan-chapters-from-video.mjs",
    "yt:scan-chapters:batch": "node ./scripts/scan-chapters-batch.mjs",
    "yt:download-scan": "node ./scripts/download-scan-chapters.mjs",
    "yt:download-scan:all": "node ./scripts/download-scan-chapters-all.mjs",
    "yt:ai-chapters": "node ./scripts/ai-chapter-detection.mjs",
    "yt:ai-chapters:all": "node ./scripts/ai-chapter-detection-all.mjs"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.4.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  }
}
